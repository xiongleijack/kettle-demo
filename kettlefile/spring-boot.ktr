    <sql>SELECT
	o.id AS order_id,
	o.order_code,
	o.create_time,
	o.user_id,
	o.total_amount,
	tg.`name` AS goods_name,
	toi.price,
	toi.quantity
FROM tbl_order_item toi
LEFT JOIN tbl_goods tg ON toi.goods_id = tg.id
LEFT JOIN tbl_order o ON toi.order_id = o.id
WHERE o.create_time >= '${beginTime}' AND o.create_time &lt;= '${endTime}'</sql>
